{
  "Alas": {
    "Playwright": {
      "Browser": {
        "type": "select",
        "value": "msedge",
        "option": [
          "msedge",
          "msedge-beta",
          "msedge-dev",
          "msedge-canary",
          "chrome",
          "chrome-beta",
          "chrome-dev",
          "chrome-canary"
        ]
      },
      "Headless": {
        "type": "checkbox",
        "value": false
      },
      "AutoOpenDevtools": {
        "type": "checkbox",
        "value": false
      },
      "AutoAcceptDialog": {
        "type": "checkbox",
        "value": true
      },
      "ExtensionDirectory": {
        "type": "input",
        "value": "$LOCALAPPDATA/Microsoft/Edge/User Data/Default/Extensions"
      },
      "ExtensionNames": {
        "type": "textarea",
        "value": "Ruffle"
      },
      "ExtraChromiumArgs": {
        "type": "textarea",
        "value": "--disable-blink-features=AutomationControlled\n--disable-features=IsolateOrigins,site-per-process\n--disable-infobars\n"
      }
    },
