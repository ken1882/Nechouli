        "type": "select",
        "value": "game",
        "option": [
          "game",
          "game_emulator"
        ]
      },
      "SaveError": {
        "type": "checkbox",
        "value": true
      },
      "ScreenshotLength": {
        "type": "input",
        "value": 1
      },
      "OnePushConfig": {
        "type": "textarea",
        "value": "provider: null",
        "mode": "yaml"
      }
    },
    "Optimization": {
      "ScreenshotInterval": {
        "type": "input",
        "value": 0.2,
        "display": "hide"
      },
      "CombatScreenshotInterval": {
        "type": "input",
        "value": 1.0,
        "display": "hide"
      },
      "WhenTaskQueueEmpty": {
        "type": "select",
        "value": "goto_main",
        "option": [
          "stay_there",
          "goto_main",
          "close_game"
        ]
      }
    }
  },
